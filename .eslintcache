location.pathname}\n                classNames=\"slide\"\n                timeout={1500}\n            >\n                <Switch location={location}>\n                    {currentUser === null ? (\n                        <Route exact path=\"/\">\n                            <Auth />\n                        </Route>\n                    ) : isCorrectedUser ? (\n                        <Route exact path=\"/\">\n                            <Waiting />\n                        </Route>\n                    ) : (\n                        <Route exact path=\"/\">\n                            <Quiz />\n                        </Route>\n                    )}\n                    <Route path=\"/about\">\n                        <About />\n                    </Route>\n                </Switch>\n            </CSSTransition>\n        </TransitionGroup>\n    ));\n\n    return (\n        <HashRouter>\n            <Loading />\n            <Nav user={currentUser} />\n            <TransitionRouter />\n        </HashRouter>\n    );\n};\n\nexport default AppRouter;\n","/Users/ohyeseong/Documents/like_amazing_lion/src/components/firebaseConfig.ts",[],"/Users/ohyeseong/Documents/like_amazing_lion/src/hooks/useAuth.tsx",["116"],"/Users/ohyeseong/Documents/like_amazing_lion/src/pages/Auth/Auth.tsx",[],"/Users/ohyeseong/Documents/like_amazing_lion/src/components/CustomAlert.tsx",["117","118"],"import React, { useEffect, useState } from \"react\";\nimport { Snackbar, Slide } from \"@material-ui/core\";\nimport { Alert } from \"@material-ui/lab\";\n\nimport { TransitionProps } from \"@material-ui/core/transitions\";\n\ninterface CustomAlertProps {\n    msg: string;\n    setMsg: React.Dispatch<React.SetStateAction<string>>;\n}\n\nconst CustomAlert = ({ msg, setMsg }: CustomAlertProps) => {\n    const [state, setState] = React.useState<{\n        open: boolean;\n        Transition: React.ComponentType<\n            TransitionProps & { children?: React.ReactElement<any, any> }\n        >;\n    }>({\n        open: false,\n        Transition: Slide,\n    });\n\n    const vertical: any = \"bottom\";\n    const horizontal: any = \"right\";\n\n    useEffect(() => {\n        setState({\n            ...state,\n            open: msg !== \"\",\n        });\n    }, [msg]);\n\n    const onClose = () => {\n        setState({\n            ...state,\n            open: false,\n        });\n\n        setMsg(\"\");\n    };\n\n    return (\n        <Snackbar\n            open={state.open}\n            onClose={onClose}\n            TransitionComponent={state.Transition}\n            key={state.Transition.name}\n            anchorOrigin={{ vertical, horizontal }}\n        >\n            <Alert onClose={onClose} severity=\"warning\">\n                {msg}\n            </Alert>\n        </Snackbar>\n    );\n};\n\nexport default CustomAlert;\n","/Users/ohyeseong/Documents/like_amazing_lion/src/components/SocialAuth.tsx",[],"/Users/ohyeseong/Documents/like_amazing_lion/src/pages/About/About.tsx",[],"/Users/ohyeseong/Documents/like_amazing_lion/src/components/Theme.ts",[],"/Users/ohyeseong/Documents/like_amazing_lion/src/components/Nav.tsx",[],"/Users/ohyeseong/Documents/like_amazing_lion/src/pages/Quiz/Quiz.tsx",[],"/Users/ohyeseong/Documents/like_amazing_lion/src/components/Loading.tsx",[],"/Users/ohyeseong/Documents/like_amazing_lion/src/components/Quizzes.ts",[],"/Users/ohyeseong/Documents/like_amazing_lion/src/components/QuizName.tsx",[],"/Users/ohyeseong/Documents/like_amazing_lion/src/components/QuizSelect.tsx",[],"/Users/ohyeseong/Documents/like_amazing_lion/src/components/QuizSlider.tsx",["119"],"/Users/ohyeseong/Documents/like_amazing_lion/src/components/QuizLastSlide.tsx",["120"],"import React, { useState } from \"react\";\nimport { Button } from \"@material-ui/core\";\n\nimport QuizName from \"components/QuizName\";\nimport { QuizSelectInterface } from \"components/QuizSelect\";\nimport { QuizInterface } from \"components/Quizzes\";\n\ninterface QuizLastSlidePro