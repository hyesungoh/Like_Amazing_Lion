tring;\n    setMsg: React.Dispatch<React.SetStateAction<string>>;\n}\n\nconst CustomAlert = ({ msg, setMsg }: CustomAlertProps) => {\n    const [state, setState] = React.useState<{\n        open: boolean;\n        Transition: React.ComponentType<\n            TransitionProps & { children?: React.ReactElement<any, any> }\n        >;\n    }>({\n        open: false,\n        Transition: Slide,\n    });\n\n    const vertical: any = \"bottom\";\n    const horizontal: any = \"right\";\n\n    useEffect(() => {\n        setState({\n            ...state,\n            open: msg !== \"\",\n        });\n    }, [msg]);\n\n    const onClose = () => {\n        setState({\n            ...state,\n            open: false,\n        });\n\n        setMsg(\"\");\n    };\n\n    return (\n        <Snackbar\n            open={state.open}\n            onClose={onClose}\n            TransitionComponent={state.Transition}\n            key={state.Transition.name}\n            anchorOrigin={{ vertical, horizontal }}\n        >\n            <Alert onClose={onClose} severity=\"warning\">\n                {msg}\n            </Alert>\n        </Snackbar>\n    );\n};\n\nexport default CustomAlert;\n","/Users/ohyeseong/Documents/like_amazing_lion/src/components/SocialAuth.tsx",[],"/Users/ohyeseong/Documents/like_amazing_lion/src/pages/About/About.tsx",[],"/Users/ohyeseong/Documents/like_amazing_lion/src/components/Theme.ts",[],"/Users/ohyeseong/Documents/like_amazing_lion/src/components/Nav.tsx",[],"/Users/ohyeseong/Documents/like_amazing_lion/src/pages/Quiz/Quiz.tsx",[],"/Users/ohyeseong/Documents/like_amazing_lion/src/components/Loading.tsx",[],"/Users/ohyeseong/Documents/like_amazing_lion/src/components/Quizzes.ts",[],"/Users/ohyeseong/Documents/like_amazing_lion/src/components/QuizName.tsx",[],"/Users/ohyeseong/Documents/like_amazing_lion/src/components/QuizSelect.tsx",[],"/Users/ohyeseong/Documents/like_amazing_lion/src/components/QuizSlider.tsx",["121"],"/Users/ohyeseong/Documents/like_amazing_